    filter: brightness(1.5) hue-rotate(180deg); }

/* ===== HEADER SECTION ===== */
.header {
  grid-area: header;
  position: relative;
  background: #B3B0AB;
  color: #fff;
  text-transform: uppercase;
  width: calc(100% + 40px);
  left: -40px; }

/* ===== HEADER ===== */
.header_image-home {
  width: calc(100% - 40px);
  margin-left: 40px;
  margin-bottom: -3px; }

.triangle-punch-out {
  border-width: 18px 42px 0 0;
  border-color: #B3B0AB transparent transparent transparent;
  position: absolute; }

.tagline {

  .social-links img:hover {
    -webkit-filter: brightness(1.5) hue-rotate(180deg);
    filter: brightness(1.5) hue-rotate(180deg); }

/* ===== HEADER SECTION ===== */
.header {
  grid-area: header;
  position: relative;
  background: #B3B0AB;
  color: #fff;
  text-transform: uppercase;
  width: calc(100% + 40px);
  left: -40px; }

/* ===== HEADER ===== */
.header_image-home {

.event-listing .events_thumbnail {
  width: 40%;
  margin-top: 5px;
  margin-bottom: 50px; }

.event-listing .events_event-title {
  font-size: 26px;
  font-weight: 700;
  margin-bottom: 25px; }

.event-listing .events_preview_date {
  font-size: 17px;
  line-height: 1.3; }

.event-listing .preview_summary {
  display: inline-block;
  font-size: 17px;
  line-height: 1.4;
  margin-top: 10px; }

.grid-single {
  grid-template-columns: 60% auto;
  grid-template-rows: auto;
  grid-gap: 35px;
  grid-template-areas: "header header" "primary sidebar"; }

.header_image-holder {

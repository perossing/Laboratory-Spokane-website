  margin-top: -15px; }

.flex-wrapper {
  margin: 0 auto;
  width: 100%;
  grid-area: flex-wrapper;
  display: flex;
  flex-flow: row wrap;
  align-items: stretch;
  align-content: stretch; }

.list_block {

/* ===== HEADER NAV ===== */
.header_nav-logo-container {
  width: calc(100% - 40px);
  padding: 5px 0 10px 10px;
  position: absolute;
  left: 40px;
  background: linear-gradient(180deg, rgba(27, 31, 38, 0.8) 20px, rgba(27, 31, 38, 0) 70px);
  z-index: 100; }

.header_logo {
  width: 200px;
  height: auto;
  margin-right: 20px; }

.header_nav {
  display: inline-block;
  position: relative;
  bottom: 25px;
  height: 25px;
  margin-top: 30px;
  margin-right: 50px;
  font-size: 16px;
  font-weight: 600; }

.header_nav_menu-item {
  position: relative; }

.header_nav_menu-item:hover {
  color: #deab63; }

.header_nav div {
  display: inline-block; }

.header_nav_divider {
  margin: 0 2px -12px 2px;
  width: 18px;
  height: 32px; }

.triangle_drop-down {
  border-width: 7px 4px 0 4px;
  border-color: #aaa transparent transparent transparent;
  margin: 0 0 3px 10px; }

/* ===== SUB-NAV ===== */
.nav_subnav {
  opacity: 0;
  position: absolute;
  top: 0;
  padding-top: 30px;
  right: -10px;
  -webkit-transition: opacity 0.2s ease-in;
  transition: opacity 0.2s ease-in;
  z-index: 100; }
  .nav_subnav:hover {
    opacity: 1; }

.subnav_menu-item {
  display: block;
  height: 30px;
  width: 130px;
  text-transform: capitalize;
  font-size: 15px;
  font-weight: 400;
  text-align: left;
  background: rgba(100, 100, 105, 0.85);
  margin: 1px 0;
  padding: 6px 0 0 8px;
  color: #fff; }
  .subnav_menu-item:hover {
    background: rgba(70, 70, 85, 0.9);
    border-bottom: solid 2px #004B80;
    color: #fab047; }

/* ===== CALENDAR SECTION ===== */
.calendar-section {
  grid-area: calendar; }

.calendar_month {
  text-transform: uppercase;
  color: #fff;
  transform-origin: 0 0;
  transform: rotate(-90deg);
  font-size: 24px;
  text-align: right;
  position: absolute;
  top: 215px;
  left: 6px;
  font-weight: 600;
  width: 200px;
  word-spacing: 3px; }

.event-calendar {
  display: block;
  position: relative;
  top: -35px;
  margin: 0 auto;
  width: 380px;
  height: 235px; }

table {
  width: 100%;
  height: 100%;
  border-collapse: collapse; }

th {
  background: #7C95BF;
  width: 11%; }

.header_top-row {
  border-top: solid 4px #fff; }

td {
  border: solid 4px #fff;
  background: #D9D5D0;
  width: 12%;
  height: 20%; }

.highlight {
  background: #E6AC5C; }

/* ===== EVENT PREVIEW LIST SECTION ===== */
.events_list {
  grid-area: sidebar; }

.events_preview {
  font-size: 16px;
  clear: both;
  overflow: hidden;
  margin-bottom: 15px;
  border-bottom: solid 1px #666; }

.events_preview:last-of-type {
  border: none; }

.preview_link-more {
  float: right;
  margin: 5px 10% 25px 0;
  font-size: 15px; }

.events_thumbnail {
  width: 30%;
  min-width: 75px;
  height: auto;
  float: left;
  margin: 0 15px 15px 0; }

.events_event-title {
  font-size: 20px; }

.events_preview_date {
  font-weight: 600;
  margin: 15px 0 10px 0; }

.events_preview_location,
.events_preview_address {
  margin: 5px 0; }

.preview_summary {

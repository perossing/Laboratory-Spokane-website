.grid-list {
  grid-template-columns: auto;
  grid-template-rows: auto;
  grid-gap: 35px;
  grid-template-areas: "header" "subhead " "listings";
  width: 100%; }

.subhead_type {
  width: 100%;
  grid-area: subhead;
  display: inline-block;
  margin: 0;
  height: auto;
  background: #000;
  color: #fff;

.tpo_pages {
  left: 0px;
  bottom: -18px; }

.event-info {
  margin-top: 30px; }
  .event-info a {
    font-size: 18px;
    display: block;
    margin-top: 15px; }

.event_date {
  font-weight: 700;
  font-size: 20px;
  line-height: 1.3; }

.event_venue-address {
  font-size: 18px;
  margin: 5px; }

.triangle-corner {
  border-width: 0 250px 85px 0;
  border-color: transparent #fff transparent transparent;
  position: absolute;
  top: 0;
  right: 0; }

.link_back_events {
  position: absolute;
  top: 15px;
  right: 25px;
  font-weight: 600;
  font-size: 16px; }

.triangle_back {
  border-width: 6px 13px 6px 0;
  border-color: transparent #004B80 transparent transparent;
  position: absolute;
  top: 4px;
  right: 65px; }

.copy-primary h2 {
  font-size: 28px;
  font-weight: 700;
  line-height: 24px; }

.copy-primary img {
  margin: 30px 25px 28px 0;
  float: left;
  max-height: 500px;
  max-width: 400px;
  width: auto;
